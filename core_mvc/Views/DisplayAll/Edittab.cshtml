@model core_mvc.Models.EmployeeInsert
@using (Html.BeginForm("updatetab", "DisplayAll", new { id = Model.emp_id }, FormMethod.Post))
{
    @Html.HiddenFor(Model=>Model.emp_id)
    <div class="form-group">
        <label class="control-label">Employee Name</label>
        <input asp-for="emp_name" class="form-control"/>
    </div>

    <div class="form-group">
        <label class="control-label">Employee Address</label>
        <input asp-for="emp_address" class="form-control" />
    </div>

    <div class="form-group">
        <label class="control-label">Employee Salary</label>
        <input asp-for="emp_salary" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Update" class="btn btn-success"/>
    </div>
}
