@model core_mvc.Models.EmployeeInsert
<form asp-action="index_click"class="form-horizondal" method="post">
    <div class="form-group">
        <label class="control-label">Employee Name</label>
        <input type="text" asp-for="emp_name" class="form-control" id="t1" />
    </div>

    <div class="form-group">
        <label class="control-label">EMployee Address</label>
        <input type="text" asp-for="emp_address" class="form-control" id="t2" />
    </div>

    <div class="form-group">
        <label class="control-label">Employee Salary</label>
        <input type="text" asp-for="emp_salary" class="form-control" id="t3" />
    </div>

    <div class="form-group">
        <input type="submit" value="Insert" class="btn btn-success" />
    </div>
</form>
@{
    if(TempData["msg"]!=null)
    {
        <script>
        alert('@TempData["msg"]');
        document.getElementById('t1').value="";
            document.getElementById('t2').value="";
                document.getElementById('t3').value="";
        </script>
    }
}
